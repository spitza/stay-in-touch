<%= form_for(@meeting) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <div class="field">
    <%= f.email_field :invitee %>
    <%= f.select :frequency, [1,2,4]%>
    <%= check_box_tag(:work) %>
    <%= label_tag(:work, "work hours") %>
  </div>
  <%= f.submit "Create", class: "btn btn-primary" %>
<% end %>